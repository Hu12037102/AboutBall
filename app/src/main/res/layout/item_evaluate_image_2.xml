<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.makeramen.roundedimageview.RoundedImageView
        android:id="@+id/riv_1"
        android:layout_width="@dimen/size_90dp"
        android:layout_height="@dimen/size_65dp"
        app:riv_oval="false"
        app:riv_corner_radius="@dimen/size_6dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:riv_border_color="@color/colorFFDCDCDC"
        app:riv_border_width="@dimen/size_1dp"
        android:scaleType="centerCrop"
        />
    <com.makeramen.roundedimageview.RoundedImageView
        android:id="@+id/riv_2"
        android:layout_width="@dimen/size_90dp"
        android:layout_height="@dimen/size_65dp"
        app:riv_oval="false"
        app:riv_corner_radius="@dimen/size_6dp"
        app:layout_constraintLeft_toRightOf="@id/riv_1"
        android:layout_marginLeft="@dimen/size_5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:riv_border_color="@color/colorFFDCDCDC"
        app:riv_border_width="@dimen/size_1dp"
        android:scaleType="centerCrop"
        />
</androidx.constraintlayout.widget.ConstraintLayout>